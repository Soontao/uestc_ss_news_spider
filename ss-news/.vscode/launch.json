{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "development",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/www/development.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy",
                "--max_old_space_size=80",
                "--max_executable_size=130"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "sourceMaps": true,
            "outDir": null
        },
        {
            "name": "production",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/www/production.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "production"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": null
        },
        {
            "name": "附加",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        },
        {
            "name": "Attach to Process",
            "type": "node",
            "request": "attach",
            "processId": "${command.PickProcess}",
            "port": 5858,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}